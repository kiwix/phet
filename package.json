    "babel-register": "^6.26.0",
    "babelify": "^10.0.0",
    "browserify": "16.2.3",
    "cheerio": "^0.22.0",
    "cli-progress": "^3.6.0",
    "dotenv": "^8.2.0",
    "glob": "^7.1.6",
